<% if(item.prev_link || item.next_link) { %>
    <div class="row">
        <div class="col-sm-12">
            <div class="wrap-pagination">
                <a class="<%- item.prev_link ? "" : "disabled" %>" href="<%- config.root %><%- item.prev_link %>">
                    <i class="fa fa-chevron-left" aria-hidden="true"></i>
                </a>
                <a class="<%- item.next_link ? "" : "disabled" %>" href="<%- config.root %><%- item.next_link %>">
                    <i class="fa fa-chevron-right" aria-hidden="true"></i>
                </a>
            </div>
        </div>
    </div>
<% } %>
